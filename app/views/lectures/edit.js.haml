$(".panel").fadeIn("slow");
$(".post_lecture_modal").fadeIn("slow");
$("#header").fadeOut("slow");

$(".panel").click(
function(){
$(this).fadeOut("slow");
$(".post_lecture_modal").fadeOut("slow");
$("#header").fadeIn("slow");
})

$(".modal-content").on('click', function(e) {
e.stopPropagation();
});
